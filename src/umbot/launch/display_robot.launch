<?xml version="1.0"?>
<launch>
  <arg name="gui" default="true" />
  <param name="use_sim_time" value="false" />
  <param name="robot_description" textfile="$(find umbot)/urdf/mybot.urdf" />
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
  <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui"
    type="joint_state_publisher_gui" if="$(arg gui)" />
  <node pkg="robot_state_publisher" name="robot_state_publisher" type="robot_state_publisher" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find umbot)/rviz/urdf.rviz" />
</launch>
